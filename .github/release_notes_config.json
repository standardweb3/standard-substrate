{
	"categories": [
		{
			"title": "## 🚀 Features",
			"labels": ["feature"]
		},
		{
			"title": "## 🐛 Fixes",
			"labels": ["bugfix"]
		},
		{
			"title": "## 🧪 Tests",
			"labels": ["test"]
